    "configuration": [{
            "default": "http://www.pollens.fr",
            "description": "URL source vigipollens web site.",
            "key": "httpsource",
            "name": "http vigipollens RNSA",
            "required": "yes",
            "type": "string"
        }
    ],

    "device_types": {
        "vigiallergen.pollens-rnsa-dep": {
            "description": "Vigilance des pollens pour un département Français",
